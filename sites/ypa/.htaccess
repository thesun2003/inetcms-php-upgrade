DefaultLanguage ru
AddDefaultCharset windows-1251
php_value default_charset "cp1251"

RewriteEngine On

RewriteCond %{HTTP_HOST} ^ypa\.ru [NC]
RewriteRule ^(.*) http://www.ypa.ru/$1 [R=301,L]

RewriteRule ^spetialization /korporativnye_prazdniki [R=301]

RewriteRule ^([a-z]+)/([0-9]+)/([0-9]+) /?t=$1&c=$2&p=$3 
RewriteRule ^([a-z]+)/year-([0-9]{4})$ /?cat=$1&year=$2 
RewriteRule ^([a-z]+)/list-([0-9]+)$ /?cat=$1&list=$2 
RewriteRule ^([a-z]+)/([a-zA-Z0-9]+)$ /?cat=$1&pag=$2 

RewriteRule ^corporation /korporativ [R=301]
RewriteRule ^korporativnye-prazdniki /korporativnye_prazdniki [R=301]
RewriteRule ^korporativnye_prazdniki /?cat=spetialization&pag=event [L]

RewriteCond %{REQUEST_FILENAME} !-d 
RewriteRule ^([a-z_]+)$ /?cat=$1 